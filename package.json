{
  "name": "rev-static",
  "version": "2.7.0",
  "description": "Static asset revisioning by appending content hash to filenames, then changing the names in html files.",
  "main": "dist/index.js",
  "scripts": {
    "lint": "tslint \"src/index.ts\" && npm run standard",
    "demo-test": "./bin/rev-static",
    "commonjs-test": "node tests/commonjs.js",
    "build": "rimraf dist demo/*-*.* demo/variables.* && tsc -p src",
    "standard": "standard \"**/*.config.js\"",
    "fix": "standard --fix \"**/*.config.js\"",
    "test": "tsc -p spec && jasmine"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/plantain-00/rev-static.git"
  },
  "keywords": [
    "rev",
    "static",
    "version",
    "sri",
    "subresource integrity"
  ],
  "author": "York Yao",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/plantain-00/rev-static/issues"
  },
  "homepage": "https://github.com/plantain-00/rev-static#readme",
  "devDependencies": {
    "@types/camelcase": "0.0.30",
    "@types/ejs": "2.3.33",
    "@types/glob": "5.0.30",
    "@types/jasmine": "2.5.53",
    "@types/minimist": "1.2.0",
    "jasmine": "2.6.0",
    "pretty-bytes": "4.0.2",
    "rimraf": "2.6.1",
    "standard": "10.0.2",
    "tslint": "5.4.3",
    "typescript": "2.4.1"
  },
  "dependencies": {
    "camelcase": "4.1.0",
    "ejs": "2.5.6",
    "glob": "7.1.2",
    "lodash.flatten": "4.4.0",
    "lodash.uniq": "4.5.0",
    "minimist": "1.2.0"
  },
  "types": "./dist/index.d.ts",
  "bin": {
    "rev-static": "./bin/rev-static"
  },
  "engines": {
    "node": ">=4"
  }
}
